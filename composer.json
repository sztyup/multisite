{
  "name": "sztyup/nexus",
  "description": "Multisite plugin for Laravel 5",
  "license": "LGPL-3.0-only",
  "authors": [{
    "name": "Csupity László",
    "email": "sztyup@gmail.com"
  }],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "laravel/framework": "^5.5",
    "laravelcollective/html": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^1.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Sztyup\\Nexus\\": "src/Nexus"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sztyup\\Nexus\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sztyup\\Nexus\\NexusServiceProvider"
      ]
    }
  }
}
